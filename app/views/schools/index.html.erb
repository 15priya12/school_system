<h1>Schools</h1>

<table border="1">
  <thead>
    <tr>
      <th>Username</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @schools.each do |school| %>
      <tr>
        <td><%= school.username %></td>
        <td>
          <a href="#">View</a> | 
          <a href="#">Delete</a>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to "Create School", new_school_path, class: "button" %>
