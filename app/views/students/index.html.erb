<h1>Students</h1>

<table border="1">
  <thead>
    <tr>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <% @students.each do |student| %>
      <tr>
        <td><%= student.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to "Create Student", new_student_path, class: "button" %>
